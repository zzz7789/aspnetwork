@model ASPwork.Models.RegisterViewModel
@{
    ViewBag.Title = "注册";
}

<h2>@ViewBag.Title。</h2>

@using (Html.BeginForm("Res", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
   
    <h4>创建新帐户。</h4>
    <hr /><span>用户名：</span>
   
   @Html.TextBox("username")
    <br>
    <br>
    <span>密码：</span>
  
    @Html.TextBox("password")
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="注册" />
        </div>
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
